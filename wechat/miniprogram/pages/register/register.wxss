.container {
    padding-top: 80rpx;
}

.lic-tmp {
    width: 600rpx;
    height: 450rpx;
    position: relative;
    display: flex;
    justify-content: center;
    align-items: center;
}

.stretch {
    width: 100%;
    height: 100%;
}

.lic-tmp-img {
    position: absolute;
    opacity: .2;
}

.instruction {
    position: absolute;
    bottom: -90rpx;
    font-size: 40rpx;
}

.cuIcon-camerafill {
    font-size: 220rpx;
}

.cu-form-group .title {
    min-width: calc(4em + 30rpx);
}

.lic-form {
    width: 600rpx;
}

.check {
    position: absolute;
    right: 0;
    top: 0;
    width: 100rpx;
    height: 100rpx;
}